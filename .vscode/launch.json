{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**/*.js",
      ],
      "smartStep": true,
      "trace": true,
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "preLaunchTask": "npm: build-clean",
      "program": "${workspaceFolder}/dist/index.js",
      "args": [
        "crawler",
        "england rugby-league super-league 2024"
      ] // Replace "index.js" with your main Node.js file
    }
  ]
}
